{
  "name": "@pitchpro/reference-tones",
  "version": "1.0.0",
  "description": "Reference tone playback library for pitch training applications using Tone.js",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "public"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "pitch",
    "audio",
    "tone",
    "reference-tone",
    "music-training",
    "pitch-training",
    "salamander-piano"
  ],
  "author": "PitchPro Team",
  "license": "MIT",
  "dependencies": {
    "tone": "^15.1.22"
  },
  "devDependencies": {
    "@types/node": "^22.7.5",
    "typescript": "^5.6.3",
    "vite": "^5.4.8",
    "vite-plugin-dts": "^4.2.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kiyopi/pitchpro-reference-tones.git"
  },
  "bugs": {
    "url": "https://github.com/kiyopi/pitchpro-reference-tones/issues"
  },
  "homepage": "https://github.com/kiyopi/pitchpro-reference-tones#readme"
}
